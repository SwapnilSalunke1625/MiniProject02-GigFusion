{
  "name": "resource-connect",
  "version": "1.0.0",
  "description": "A web application to connect people with home service providers.",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run start:server\" \"npm run start:client\"",
    "start:server": "cd backend && npm run dev",
    "start:client": "cd frontend && npm run dev",
    "setup": "concurrently \"npm run install:server\" \"npm run install:client\"",
    "install:server": "cd ./backend/ && npm i",
    "install:client": "cd ./frontend/ && npm i"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^9.0.1",
    "daisyui": "^4.12.14"
  },
  "dependencies": {
    "@emotion/react": "^11.13.5",
    "@emotion/styled": "^11.13.5",
    "@mui/icons-material": "^6.1.9",
    "@mui/material": "^6.1.9",
    "nodemailer": "^6.9.16",
    "path": "^0.12.7",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^5.4.0",
    "react-toastify": "^10.0.6",
    "react-top-loading-bar": "^2.3.1",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "url": "^0.11.4",
    "zustand": "^5.0.1"
  }
}
